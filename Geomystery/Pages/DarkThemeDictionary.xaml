<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Geomystery.Pages">
    <!--夜间模式-->
    
    <!--暗紫色背景-->
    <SolidColorBrush x:Key="SystemBackgroundAltHighBrush" Color="#FF352D3A"/>
    <!--浅橙色背景-->
    <SolidColorBrush x:Key="SystemBackgroundBaseHighBrush" Color="#FFFEF2DA"/>
    <!--浅紫色文字-->
    <SolidColorBrush x:Key="TextColorPurple" Color="#FFB08AFF"/>
    <!--粉紫色渐变-->
    <LinearGradientBrush x:Key="ColorGradient1" StartPoint="0,0" EndPoint="1,1">
        <GradientStop Color="#ffc954bf" Offset="0.0" />
        <GradientStop Color="#ff825be6" Offset="1.0" />
    </LinearGradientBrush>
    
    <Color x:Key="SystemTranslucentBaseHighColor">#FFFFFFFF</Color>
    <Color x:Key="SystemThemeMainColor">#FF0074CE</Color>

</ResourceDictionary>