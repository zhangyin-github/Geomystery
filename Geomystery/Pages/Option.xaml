                        <Setter Target="SFXVolSlider.Style" Value="{StaticResource SliderStyle1}"/>
                        <Setter Target="SFXVolSlider.Width" Value="400"/>
                        <Setter Target="Fullscreen.FontSize" Value="36"/>
                        <Setter Target="Fullscreen.Height" Value="72"/>
                        <Setter Target="Fullscreen.Width" Value="72"/>
                        <Setter Target="Fullscreen.Margin" Value="12,0,0,0"/>
                        <Setter Target="Fullscreen.Style" Value="{StaticResource ToggleButtonStyle1}"/>
                        <Setter Target="NightMode.FontSize" Value="36"/>
                        <Setter Target="NightMode.Height" Value="72"/>
                        <Setter Target="NightMode.Width" Value="72"/>
                        <Setter Target="NightMode.Margin" Value="12,0,0,0"/>
                        <Setter Target="NightMode.Style" Value="{StaticResource ToggleButtonStyle1}"/>
                        <Setter Target="ResetProgress.FontSize" Value="36"/>
                        <Setter Target="ResetProgress.Height" Value="72"/>
                        <Setter Target="ResetProgress.Width" Value="72"/>
                        <Setter Target="ResetProgress.Margin" Value="12,0,0,0"/>
                        <Setter Target="ResetProgress.Style" Value="{StaticResource ButtonStyle1}"/>
                        <Setter Target="AboutUs.FontSize" Value="44"/>
                    </VisualState.Setters>
                </VisualState>
            </VisualStateGroup>
        </VisualStateManager.VisualStateGroups>

        
        <Grid Name="OptionGrid" Background="{x:Null}" Height="720">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="1*"/>
                <ColumnDefinition Width="1*"/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="1*"/>
                <RowDefinition Height="1*"/>
                <RowDefinition Height="1*"/>
                <RowDefinition Height="1*"/>
                <RowDefinition Height="1*"/>
                <RowDefinition Height="1*"/>
                <RowDefinition Height="1*"/>
            </Grid.RowDefinitions>

            <TextBlock Name="LanguageT"
                       Grid.Column="0" Grid.Row="0"
                       FontFamily="Microsoft YaHei UI" FontSize="44" Foreground="{ThemeResource TextColorPurple}"
                       Margin="0,0,36,0" HorizontalAlignment="Right" VerticalAlignment="Center"
                       Text="Language" x:Uid="Language"/>
            <TextBlock Name="MusicVolumeT"
                       Grid.Column="0" Grid.Row="1"
                       FontFamily="Microsoft YaHei UI" FontSize="44" Foreground="{ThemeResource TextColorPurple}"
                       Margin="0,0,36,0" HorizontalAlignment="Right" VerticalAlignment="Center"
                       Text="Music Volume" x:Uid="Music Volume"/>
            <TextBlock Name="SFXVolumeT"
                       Grid.Column="0" Grid.Row="2"
                       FontFamily="Microsoft YaHei UI" FontSize="44" Foreground="{ThemeResource TextColorPurple}"
                       Margin="0,0,36,0" HorizontalAlignment="Right" VerticalAlignment="Center"
                       Text="SFX Volume" x:Uid="SFX Volume"/>
            <TextBlock Name="FullscreenT"
                       Grid.Column="0" Grid.Row="3"
                       FontFamily="Microsoft YaHei UI" FontSize="44" Foreground="{ThemeResource TextColorPurple}"
                       Margin="0,0,36,0" HorizontalAlignment="Right" VerticalAlignment="Center"
                       Text="Fullscreen" x:Uid="Fullscreen"/>
            <TextBlock Name="NightModeT"
                       Grid.Column="0" Grid.Row="4"
                       FontFamily="Microsoft YaHei UI" FontSize="44" Foreground="{ThemeResource TextColorPurple}"
                       Margin="0,0,36,0" HorizontalAlignment="Right" VerticalAlignment="Center"
                       Text="Night Mode" x:Uid="Night Mode"/>
            <TextBlock Name="ResetT"
                       Grid.Column="0" Grid.Row="5"
                       FontFamily="Microsoft YaHei UI" FontSize="44" Foreground="{ThemeResource TextColorPurple}"
                       Margin="0,0,36,0" HorizontalAlignment="Right" VerticalAlignment="Center"
                       Text="Reset Progress" x:Uid="Reset Progress"/>
            <TextBlock Name="AboutT"
                       Grid.Column="0" Grid.Row="6"
                       FontFamily="Microsoft YaHei UI" FontSize="44" Foreground="{ThemeResource TextColorPurple}"
                       Margin="0,0,36,0" HorizontalAlignment="Right" VerticalAlignment="Center"
                       Text="About Us" x:Uid="About Us"/>

            <StackPanel Name="LanPanel"
                        Orientation="Horizontal" 
                        Grid.Column="1" Grid.Row="0">
                <Button Name="LanLeft"
                        FontFamily="Segoe UI Symbol" FontSize="44" Foreground="{ThemeResource TextColorPurple}" Content="&#x25C0;"
                        Background="{x:Null}" Style="{StaticResource ButtonStyleRL}"
                        Click="LanLeft_Click" IsTabStop="False"/>
                <TextBlock Name="LanguageNow"
                           FontFamily="Microsoft YaHei UI" FontSize="44" Foreground="{ThemeResource TextColorPurple}"
                           Width="200" VerticalAlignment="Center"
                           Text="English" TextAlignment="Center" x:Uid="LanguageNow"/>
                <Button Name="LanRight"
                        FontFamily="Segoe UI Symbol" FontSize="44" Foreground="{ThemeResource TextColorPurple}" Content="&#x25B6;"
                        Background="{x:Null}" Style="{StaticResource ButtonStyleRL}"
                        Click="LanRight_Click" IsTabStop="False"/>
            </StackPanel>
            <Slider Name="MusicVolSlider"
                    Grid.Column="1" Grid.Row="1"
                    Style="{StaticResource SliderStyle1}"
                    Background="{ThemeResource SystemBackgroundBaseHighBrush}"
                    HorizontalAlignment="Left" VerticalAlignment="Center"
                    Width="400" IsTabStop="False" ValueChanged="MusicVolSlider_ValueChanged"/>
            <Slider Name="SFXVolSlider"
                    Grid.Column="1" Grid.Row="2" 
                    Style="{StaticResource SliderStyle1}"
                    Background="{ThemeResource SystemBackgroundBaseHighBrush}"
                    HorizontalAlignment="Left" VerticalAlignment="Center"
                    Width="400" IsTabStop="False" ValueChanged="SFXVolSlider_ValueChanged"/>
            <ToggleButton Name="Fullscreen"
                          Grid.Column="1" Grid.Row="3" 
                          Style="{StaticResource ToggleButtonStyle1}"
                          FontFamily="Segoe UI Emoji" FontSize="36" Content="&#x2714;"
                          Background="{x:Null}"
                          Height="72" Width="72" Margin="12,0,0,0"
                          Click="Fullscreen_Click" IsTabStop="False"/>
            <ToggleButton Name="NightMode"
                          Grid.Column="1" Grid.Row="4" 
                          Style="{StaticResource ToggleButtonStyle1}"
                          FontFamily="Segoe UI Emoji" FontSize="36" Content="&#x2716;"
                          Background="{x:Null}"
                          Height="72" Width="72" Margin="12,0,0,0"
                          VerticalAlignment="Center"
                          Click="NightMode_Click" IsTabStop="False"/>
            <Button Name="ResetProgress"
                    Grid.Column="1" Grid.Row="5" 
                    Style="{StaticResource ButtonStyle1}"
                    FontFamily="Segoe UI Symbol" FontSize="36" Foreground="#FFD42200" Content="&#xE117;"
                    Background="{x:Null}"
                    Height="72" Width="72" Margin="12,0,0,0"
                    Click="ResetProgress_Click" IsTabStop="False"/>
            <Button Name="AboutUs"
                    Grid.Column="1" Grid.Row="6" 
                    Style="{StaticResource ButtonStyleRL}"
                    FontFamily="Microsoft YaHei UI" FontSize="44" Content="Geomystery"
                    Background="{x:Null}"
                    Foreground="{ThemeResource TextColorPurple}"
                    Click="AboutUs_Click" IsTabStop="False"/>
        </Grid>
    </Grid>
</Page>